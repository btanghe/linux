			/* 44 */	"",		/* 45 */	"R27",		/* 46 */	"IC1-6",	/* 47 */	"S8-10",
			/* 48 */	"R48",		/* 49 */	"R47",		/* 50 */	"R49",		/* 51 */	"R50",
			/* 52 */	"R45",		/* 53 */	"R46";

			groups = /*	GPIO_I		GPIO_O	ALT0			ALT1		ALT2		ALT3		ALT4		ALT5	*/
			/*  0 */	"P1",		"",	"BSC0",			"",		"",		"",		"",		"",
			/*  1 */	"P1",		"",	"BSC0",			"",		"",		"",		"",		"",
