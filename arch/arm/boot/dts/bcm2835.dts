			/* 47 */	"",		"",		"",		"SD_CARD_DET",		"",		"",

			/* 48 */	"",		"",		"",		"SD_CLK_R",		"",		"",
			/* 49 */	"",		"",		"",		"SD_CMD_R",		"",		"",
