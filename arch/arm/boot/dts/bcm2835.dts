			/* 26 */	"",		"",		"",		"SD1_DAT2",		"ARM_TDI",	"",
			/* 27 */	"",		"",		"",		"SD1_DAT3",		"ARM_TMS",	"",

			/* 28 */	"SDA0",		"SA5",		"PCM_CLK",	"",			"",		"",
			/* 29 */	"SCL0",		"SA4",		"PCM_FS",	"",			"",		"",
			/* 30 */	"",		"SA3",		"PCM_DIN",	"UART0_CTS",		"",		"UART1_CTS",
			/* 31 */	"",		"SA2",		"PCM_DOUT",	"UART0_RTS",		"",		"UART1_RTS",

			/* 32 */	"GPCLK0",	"SA1",		"",		"UART0_TXD",		"",		"UART1_TXD",
