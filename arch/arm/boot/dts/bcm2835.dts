			/*  4 */	"P1-07",	/*  5 */	"S5-12",	/*  6 */	"IC3-12",	/*  7 */	"P1-26",
			/*  8 */	"P1-24",	/*  9 */	"P1-21",	/* 10 */	"P1-19",	/* 11 */	"P1-23",
			/* 12 */	"",		/* 13 */	"",		/* 14 */	"P1-08",	/* 15 */	"P1-10",
			/* 16 */	"D5",		/* 17 */	"P1-11",	/* 18 */	"P1-12",	/* 19 */	"",
			/* 20 */	"",		/* 21 */	"P1-13",	/* 22 */	"P1-15",	/* 23 */	"P1-16",
			/* 24 */	"P1-18",	/* 25 */	"P1-22",	/* 26 */	"",		/* 27 */	"S5-11",
			/* 28 */	"R8+R10",	/* 29 */	"R7+R9",	/* 30 */	"R4+R6",	/* 31 */	"R3+R5",
			/* 32 */	"",		/* 33 */	"",		/* 34 */	"",		/* 35 */	"",
			/* 36 */	"",		/* 37 */	"",		/* 38 */	"",		/* 39 */	"",
