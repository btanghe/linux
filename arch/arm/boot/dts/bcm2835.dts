			/*  6 */	"",		"",	"GPCLK2",		"",		"",		"",		"",		"",
			/*  7 */	"P1",		"",	"SPI0",			"",		"",		"",		"",		"",

			/*  8 */	"P1",		"",	"SPI0",			"",		"",		"",		"",		"",
