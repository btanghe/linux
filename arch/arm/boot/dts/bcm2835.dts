			compatible = "broadcom,bcm2835-gpio", "broadcom,bcm2708-gpio";
			reg = <0x20001c 0x78>;

			interrupt-parent = <&vic2>;
			interrupts = <17 18 19 20>;

			gpio-controller;
			#gpio-cells = <2>;

			pinctrl = <&pins>;
		};

		amba {
			compatible = "arm,amba-bus";
			#address-cells = <1>;
			#size-cells = <1>;
			ranges;

			uart0 {
				compatible = "broadcom,bcm2835-uart", "broadcom,bcm2708-uart", "arm,pl011", "arm,primecell", "broadcom,bcm2835-pinctrl-device", "broadcom,bcm2708-pinctrl-device";
				reg = <0x201000 0x1000>;
				interrupt-parent = <&vic2>;
				interrupts = <25>;

				pinctrl = <&pins>;
				pinmap = "UART0";
			};
		};

