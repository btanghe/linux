			/* 18 */	"PCM_CLK",	"SD10",		"",		"BSCSL_SDA|SPISL_MOSI",	"SPI1_CE0_N",	"PWM0",
			/* 19 */	"PCM_FS",	"SD11",		"",		"BSCSL_SCL|SPISL_SCLK",	"SPI1_MISO",	"PWM1",

			/* 20 */	"PCM_DIN",	"SD12",		"",		"SPISL_MISO",		"SPI1_MOSI",	"GPCLK0",
			/* 21 */	"PCM_DOUT",	"SD13",		"",		"SPISL_CE_N",		"SPI1_SCLK",	"GPCLK1",
			/* 22 */	"",		"SD14",		"",		"SD1_CLK",		"ARM_TRST",	"",
			/* 23 */	"",		"SD15",		"",		"SD1_CMD",		"ARM_RTCK",	"",

			/* 24 */	"",		"SD16",		"",		"SD1_DAT0",		"ARM_TDO",	"",
