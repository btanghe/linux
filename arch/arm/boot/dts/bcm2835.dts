			/*  5 */	"GPCLK1",	"SA0",		"",		"",			"",		"ARM_TDO",
			/*  6 */	"GPCLK2",	"SOE_N|SE",	"",		"",			"",		"ARM_RTCK",
			/*  7 */	"SPI0_CE1_N",	"SWE_N|SRW_N",	"",		"",			"",		"",

			/*  8 */	"SPI0_CE0_N",	"SD0",		"",		"",			"",		"",
			/*  9 */	"SPI0_MISO",	"SD1",		"",		"",			"",		"",
			/* 10 */	"SPI0_MOSI",	"SD2",		"",		"",			"",		"",
			/* 11 */	"SPI0_SCLK",	"SD3",		"",		"",			"",		"",

			/* 12 */	"PWM0",		"SD4",		"",		"",			"",		"ARM_TMS",
			/* 13 */	"PWM1",		"SD5",		"",		"",			"",		"ARM_TCK",
			/* 14 */	"UART0_TXD",	"SD6",		"",		"",			"",		"UART1_TXD",
			/* 15 */	"UART0_RXD",	"SD7",		"",		"",			"",		"UART1_RXD",

			/* 16 */	"",		"SD8",		"",		"UART0_CTS",		"SPI1_CE2_N",	"UART1_CTS",
